<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>


   <servlet-mapping>
	    <servlet-name>default</servlet-name>
	    <url-pattern>*.js</url-pattern>
  </servlet-mapping>

  <servlet>
	    <servlet-name>reloadServlet</servlet-name>
	    <servlet-class>org.server.opt.services.servlet.ReloadServlet</servlet-class>
  </servlet>
  <servlet-mapping>
	    <servlet-name>reloadServlet</servlet-name>
	    <url-pattern>/reloadServlet</url-pattern>
  </servlet-mapping>
    <servlet>
	    <servlet-name>restartServlet</servlet-name>
	    <servlet-class>org.server.opt.services.servlet.RestartServlet</servlet-class>
  </servlet>
  <servlet-mapping>
	    <servlet-name>restartServlet</servlet-name>
	    <url-pattern>/restartServlet</url-pattern>
  </servlet-mapping>
  
  <servlet>
	    <servlet-name>testServlet</servlet-name>
	    <servlet-class>org.server.opt.services.servlet.TestServlet</servlet-class>
  </servlet>
  <servlet-mapping>
	    <servlet-name>testServlet</servlet-name>
	    <url-pattern>/testServlet</url-pattern>
  </servlet-mapping>

  <servlet>
	    <servlet-name>startBatServlet</servlet-name>
	    <servlet-class>org.server.opt.services.servlet.StartBatServlet</servlet-class>
  </servlet>
  <servlet-mapping>
	    <servlet-name>startBatServlet</servlet-name>
	    <url-pattern>/startBatServlet</url-pattern>
  </servlet-mapping>

  <servlet>
	    <servlet-name>remoteShellServlet</servlet-name>
	    <servlet-class>org.server.opt.services.servlet.RemoteShellServlet</servlet-class>
  </servlet>
  <servlet-mapping>
	    <servlet-name>remoteShellServlet</servlet-name>
	    <url-pattern>/remoteShellServlet</url-pattern>
  </servlet-mapping>
  
    
</web-app>



